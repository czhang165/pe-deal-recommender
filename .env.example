# PE Deal Recommender Environment Configuration
# Copy this to .env and modify as needed

# Environment (local, dev, staging, prod)
RECSYS_ENVIRONMENT=local

# Storage backend (local, s3)
RECSYS_STORAGE_BACKEND=local

# Compute backend (local, ray)
RECSYS_COMPUTE_BACKEND=local

# Paths
RECSYS_DATA_DIR=data
RECSYS_MODELS_DIR=models
RECSYS_LOGS_DIR=logs

# AWS (only needed if using S3)
# RECSYS_AWS_REGION=us-east-1
# RECSYS_S3_BUCKET=your-bucket-name

# Ray (only needed if using distributed training)
# RECSYS_RAY_ADDRESS=auto
